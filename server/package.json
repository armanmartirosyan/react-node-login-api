{
  "name": "server",
  "version": "0.1.1",
  "description": "Node.js server for user authentication.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate": "NODE_OPTIONS='--import=tsx' npx knex migrate:latest --knexfile ./src/config/knexfile.ts ",
    "rollback": "NODE_OPTIONS='--import=tsx' npx knex migrate:rollback --knexfile ./src/config/knexfile.ts ",
    "dev": "npx nodemon",
    "build": "npx rimraf ./dist && npx tsc",
    "start": "npm run build && node ./dist/index.js"
  },
  "author": "Arman Martirosyan",
  "license": "MIT",
  "dependencies": {
    "@types/uuid": "^9.0.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5"
  }
}
